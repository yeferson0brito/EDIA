Content-Type: application/json
Authorization: Bearer {{accessToken}}

{
  "date": "2026-01-21",
  "steps": 12000,
  "distance_km": 9.5,
  "calories_kcal": 600.0,
  "sleep_hours": 7.5,
  "sleep_quality": "Excelente",
  "mood": 5,
  "hydration_ml": 3000,
  "activities": [
    {
      "activity_type": "Gimnasio",
      "duration_minutes": 60
